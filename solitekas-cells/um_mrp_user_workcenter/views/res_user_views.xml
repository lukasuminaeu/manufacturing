<odoo>

    <record id="res_users_view_workcenter_form" model="ir.ui.view">
        <field name="name">res.users.view.form.inherit.workcenter</field>
        <field name="model">res.users</field>
        <field name="inherit_id" ref="base.view_users_form"/>
        <field name="arch" type="xml">
            <group name="messaging" position="after">
                <group name="mrp" string="MRP">
                    <field name="workcenter_id"/>
                </group>
            </group>
        </field>
    </record>

</odoo>